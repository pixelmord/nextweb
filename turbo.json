{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**"],
      "env": ["NEXT_PUBLIC_SITE_URL", "NODE_ENV"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": [],
      "env": ["NEXT_PUBLIC_SITE_URL", "NODE_ENV"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "env": ["NEXT_PUBLIC_SITE_URL", "NODE_ENV"]
    },
    "type-check": {
      "outputs": []
    }
  }
}
