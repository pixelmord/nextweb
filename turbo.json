{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"],
      "env": ["NEXT_PUBLIC_SITE_URL", "NODE_ENV"]
    },
    "test": {
      "outputs": ["coverage/**"],
      "dependsOn": [],
      "env": ["NEXT_PUBLIC_SITE_URL", "NODE_ENV"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "env": ["NEXT_PUBLIC_SITE_URL", "NODE_ENV"]
    },
    "type-check": {
      "outputs": []
    },
    "poolbase#build": {
      "dependsOn": ["^build"],
      "env": ["NEXT_PUBLIC_SUPABASE_URL", "NEXT_PUBLIC_SUPABASE_ANON_KEY", "NODE_ENV"],
      "outputs": [".next/**"]
    },
    "poolbase#dev": {
      "env": ["NEXT_PUBLIC_SUPABASE_URL", "NEXT_PUBLIC_SUPABASE_ANON_KEY", "NODE_ENV"],
      "cache": false
    },
    "poolbase-browser#build": {
      "dependsOn": ["^build"],
      "env": ["PLASMO_PUBLIC_SUPABASE_URL", "PLASMO_PUBLIC_SUPABASE_ANON_KEY", "NODE_ENV"],
      "outputs": [".next/**"]
    },
    "poolbase-browser#dev": {
      "env": ["PLASMO_PUBLIC_SUPABASE_URL", "PLASMO_PUBLIC_SUPABASE_ANON_KEY", "NODE_ENV"],
      "cache": false
    }
  }
}
